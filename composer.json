{
    "name": "fashionphile/laravel-kafka",
    "description": "Service Provider wrapper for laravel-kafka package for use in Fashionphile Laravel projects.",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Fashionphile\\LaravelKafka\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Eugene Bednik",
            "email": "ebednik@fashionphile.com"
        }
    ],
    "require": {
      "php": "^8.0",
      "mateusjunges/laravel-kafka": "^1.7",
      "guzzlehttp/guzzle": "^7.0"
    },
    "extra": {
        "laravel": {
          "providers": [
            "Fashionphile\\LaravelKafka\\Providers\\KafkaServiceProvider"
          ]
      }
  }
}
